# Operadores Aritmeticos

# Adição
print(1 + 1)

# subtração
print(10 - 2)

# Multiplicação
print(4 * 3)

# Divisão
print(12 / 3)

# Divisão inteira
print(12 // 3)

# Módulo
print(10 % 3)

# Exponenciação
print(2 ** 3)

# Precedência de Operadores
print(10 - 5 * 2)

print((10 -5) *2)

print(10 ** 2 * 2)

print(10 ** (2 * 2))

print(10 / 2 * 4)